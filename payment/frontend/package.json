{
    "name": "track-and-trace-front-end",
    "private": true,
    "version": "1.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --cache --max-warnings 0 --ext .ts,.tsx",
        "lint-fix": "yarn lint --fix",
        "fmt-check": "prettier --check .",
        "fmt": "prettier --write .",
        "preview": "vite preview",
        "build:contracts": "cargo concordium build  -e -o ../cis5-smart-contract-wallet/concordium-out/module.wasm.v1 -- --manifest-path ../cis5-smart-contract-wallet/Cargo.toml",
        "generate": "ccd-js-gen --module ../cis5-smart-contract-wallet/concordium-out/module.wasm.v1 --out-dir ./generated --output-type TypeScript --ts-nocheck"
    },
    "dependencies": {
        "@concordium/web-sdk": "^7.3.2",
        "@fortawesome/fontawesome-svg-core": "^6.5.2",
        "@fortawesome/free-solid-svg-icons": "^6.5.2",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@noble/ed25519": "^2.1.0",
        "@types/json-bigint": "^1.0.4",
        "@types/react-router-dom": "^5.3.3",
        "@yudiel/react-qr-scanner": "^1.2.10",
        "bootstrap": "^5.3.3",
        "buffer": "^6.0.3",
        "jdenticon": "^3.2.0",
        "json-bigint": "^1.0.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.10.1",
        "react-dom": "^18.2.0",
        "react-jdenticon": "^1.1.0",
        "react-qrcode-logo": "^2.10.0",
        "react-router-dom": "^6.22.1",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "vite-plugin-node-polyfills": "^0.21.0"
    },
    "devDependencies": {
        "@concordium/ccd-js-gen": "^1.2.0",
        "@swc-jotai/react-refresh": "^0.1.0",
        "@types/react": "^18.2.55",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.20.0",
        "@vitejs/plugin-react-swc": "^3.6.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "prettier": "^3.2.5",
        "sass": "^1.70.0",
        "typescript": "^5.0.2",
        "vite": "^5.1.1",
        "vite-plugin-handlebars": "^2.0.0",
        "vite-plugin-top-level-await": "^1.3.1",
        "vite-plugin-wasm": "^3.2.2",
        "vite-tsconfig-paths": "^4.2.1"
    }
}
