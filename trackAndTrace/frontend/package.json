{
    "name": "track-and-trace-front-end",
    "private": true,
    "version": "1.2.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --cache --max-warnings 0 --ext .ts,.tsx",
        "lint-fix": "yarn lint --fix",
        "fmt-check": "prettier --check .",
        "fmt": "prettier --write .",
        "preview": "vite preview",
        "build:contracts": "cargo concordium build  -e -o ../smart-contract/concordium-out/module.wasm.v1 -- --manifest-path ../smart-contract/Cargo.toml",
        "generate": "ccd-js-gen --module ../smart-contract/concordium-out/module.wasm.v1 --out-dir ./generated --output-type TypeScript --ts-nocheck"
    },
    "dependencies": {
        "@concordium/react-components": "^0.4.0",
        "@concordium/wallet-connectors": "^0.4.0",
        "@concordium/web-sdk": "^7.5.1",
        "@hookform/resolvers": "^3.9.1",
        "@radix-ui/react-dialog": "^1.1.2",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-select": "^2.1.2",
        "@radix-ui/react-separator": "^1.1.0",
        "@radix-ui/react-slot": "^1.1.0",
        "@radix-ui/react-switch": "^1.1.1",
        "@radix-ui/react-tooltip": "^1.1.4",
        "@uidotdev/usehooks": "^2.4.1",
        "buffer": "^6.0.3",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "cmdk": "1.0.0",
        "json-bigint": "^1.0.0",
        "leaflet": "^1.9.4",
        "lucide-react": "^0.456.0",
        "moment": "^2.30.1",
        "pinata": "^1.7.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-dropzone": "^14.3.5",
        "react-hook-form": "^7.53.2",
        "react-leaflet": "^4.2.1",
        "react-router-dom": "^6.28.0",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "tailwind-merge": "^2.5.4",
        "tailwindcss-animate": "^1.0.7",
        "vite-plugin-node-polyfills": "^0.22.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@concordium/ccd-js-gen": "^1.2.1",
        "@swc-jotai/react-refresh": "^0.1.1",
        "@types/json-bigint": "^1.0.4",
        "@types/leaflet": "^1.9.14",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react-swc": "^3.7.1",
        "autoprefixer": "^10.4.20",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.14",
        "postcss": "^8.4.49",
        "prettier": "^3.3.3",
        "sass": "^1.80.7",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.6.3",
        "vite": "^5.4.11",
        "vite-plugin-handlebars": "^2.0.0",
        "vite-plugin-top-level-await": "^1.4.4",
